<div id="<%= dom_id product %>">
  <p>
    <strong>Product:</strong>
    <%= product.id %>
  </p>

  <p>
    <strong>Subcategory:</strong>
    <%= product.subcategory_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= product.height %>
  </p>

  <p>
    <strong>Width:</strong>
    <%= product.width %>
  </p>

  <p>
    <strong>Length:</strong>
    <%= product.length %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= product.color %>
  </p>

  <p>
    <strong>Material:</strong>
    <%= product.material %>
  </p>

  <p>
    <strong>Producer:</strong>
    <%= product.producer_id %>
  </p>

  <p>
    <strong>Availability:</strong>
    <%= product.availability %>
  </p>

</div>
