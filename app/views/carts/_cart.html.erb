<div id="<%= dom_id cart %>">
  <p>
    <strong>Cart:</strong>
    <%= cart.cart_id %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= cart.product_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= cart.quantity %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= cart.price %>
  </p>

</div>
